
mod std::io

pub ext fun println[T](value: T) = "
	console.log(value);
"


mod std::String

pub ext fun concat(low: String, high: String) -> String = "
    return low + high;
"
pub ext fun slice(self: String, start: Int, end: Int) -> String = "
    return self.substring(Number(start), Number(end));
"


mod test

enum Option[T](Some: T, None)

fun greet(thing: String) -> String 
	= String::concat(
		"Hello, ",
		String::concat(thing, "!")
	)

fun add[T](a: T, b: T) -> T = a + b

fun main() {
    val test: Option[Int] = Option::Some[Int](5)
    io::println[Int](add[Int](5, 10))
    io::println[String](greet("world"))
}
