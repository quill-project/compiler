
mod std

pub fun println[T](value: T) = value 
    |> T::as_string()
    |> raw_println()

pub fun range(start: Int, end: Int) -> Sequence[Int] {
    mut n = start
    return Sequence::new(|| {
        if n >= end { return Option::None }
        n = n + 1
        return Option::Some(n - 1)
    })
}