
mod std

pub fun String::as_string(self: String) -> String = self

pub ext fun String::slice(self: String, start: Int, end: Int) -> String = "
    return self.slice(Number(start), Number(end));
"

pub ext fun String::concat(low: String, high: String) -> String = "
    return low + high;
"

pub fun String::join(from: Sequence[String], delim: String) -> String
    = from |> reduce(String::concat) |> unwrap_or("")